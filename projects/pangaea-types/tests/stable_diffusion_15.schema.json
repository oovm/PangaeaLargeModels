        {
            "description": "A struct that holds the prompts for the image generation.",
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "Stable Diffusion v1.4"
                },
                "api_token": {
                    "description": "The token owner will pay the fee",
                    "type": "integer",
                    "format": "uint64",
                    "minimum": 0
                },
                "cfg_scale": {
                    "type": "number",
                    "format": "float",
                    "default": 7.0,
                    "maximum": 10,
                    "minimum": 1
                },
                "clip_skip": {
                    "description": "The CLIP will stop at the last layers",
                    "type": "integer",
                    "format": "uint",
                    "default": 2,
                    "minimum": 0
                },
                "extension": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/DiffuserExtension"
                    }
                },
                "high_resolution": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/HighResolutionFixer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null
                },
                "negative_prompt": {
                    "description": "The negative prompt",
                    "type": "string",
                    "default": ""
                },
                "positive_prompt": {
                    "description": "The positive prompt",
                    "type": "string",
                    "default": ""
                },
                "seed": {
                    "description": "The random seed",
                    "type": "integer",
                    "format": "int64",
                    "default": 0
                },
                "steps": {
                    "type": "integer",
                    "format": "uint8",
                    "minimum": 0
                },
                "task_id": {
                    "description": "The unique id of the task",
                    "type": "integer",
                    "format": "uint64",
                    "minimum": 0
                }
            },
            "required": [
                "type",
                "task_id",
                "api_token",
                "steps",
                "extension"
            ]
        },

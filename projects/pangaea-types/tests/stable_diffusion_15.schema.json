    "$defs": {
        "DiffuserExtension": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "TextEmbedding"
                        },
                        "model_id": {
                            "type": "integer",
                            "format": "uint64",
                            "minimum": 0
                        }
                    },
                    "required": [
                        "type",
                        "model_id"
                    ]
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "LoRA"
                        },
                        "model_id": {
                            "type": "integer",
                            "format": "uint64",
                            "minimum": 0
                        },
                        "strength": {
                            "type": "number",
                            "format": "float",
                            "default": 0.800000011920929
                        }
                    },
                    "required": [
                        "type",
                        "model_id"
                    ]
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string",
                            "const": "Dora"
                        }
                    },
                    "required": [
                        "type"
                    ]
                }
            ]
        },
        "HighResolutionFixMethod": {
            "type": "string",
            "enum": [
                "EsrGan"
            ]
        },
        "HighResolutionFixer": {
            "type": "object",
            "properties": {
                "method": {
                    "$ref": "#/$defs/HighResolutionFixMethod",
                    "default": "EsrGan"
                },
                "scale": {
                    "type": "number",
                    "format": "float",
                    "default": 2.0
                }
            }
        }
    }
